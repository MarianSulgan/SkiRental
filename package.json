{
  "name": "skirental",
  "version": "1.0.0",
  "description": "SKIrental project",
  "main": "main.js",
  "scripts": {
    "start": "concurrently -k \"npm run dev-win\" \"npm run electron\"",
    "electron": "SET NODE_ENV=development&&electron . --hot",
    "clean": "rimraf dist",
    "dev-win": "SET NODE_ENV=development&&babel-node server.js",
    "build-win": "rimraf dist && SET NODE_ENV=production&& webpack --config ./webpack.production.config.js --progress --profile --colors",
    "start-win": "SET NODE_ENV=production&&SET PORT=80&&babel-node server.js",
    "build": "SET NODE_ENV=production&& webpack --config ./webpack.production.config.js --progress --profile --colors && electron-packager . SkiRental --platform=win32 --arch=x64 --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MarianSulgan/SkiRental.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "reduxjs",
    "electron"
  ],
  "dependencies": {
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.23.1",
    "electron": "^1.4.1",
    "electron-packager": "^8.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jquery": "^3.1.0",
    "jsonwebtoken": "^7.1.7",
    "jwt-decode": "^2.0.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.12.0",
    "nedb": "^1.8.0",
    "react": "^15.0.2",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-transition-group": "^15.2.1",
    "react-bootstrap": "^0.30.0",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "4.0.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.0.0",
    "superagent-defaults": "^0.1.14",
    "superagent-promise": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "concurrently": "^3.1.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "express": "^4.13.4",
    "file-loader": "^0.9.0",
    "http-proxy-middleware": "^0.17.0",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
